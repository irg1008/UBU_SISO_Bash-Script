local estilo1=("═" "╔" "╠" "╚" "╦" "╬" "╩" "╗" "╣" "╝" "║")
local estilo2=("─" "╭" "├" "╰" "┬" "┼" "┴" "╮" "┤" "╯" "│")
local estilo3=("━" "┏" "┣" "┗" "┳" "╋" "┻" "┓" "┫" "┛" "┃")
local estilo4=("─" "┌" "├" "└" "┬" "┼" "┴" "┐" "┤" "┘" "│")

case "$1" in
1)
  estiloGeneral=("${estilo1[@]}")
  ;;
2)
  estiloGeneral=("${estilo2[@]}")
  ;;
3)
  estiloGeneral=("${estilo3[@]}")
  ;;
4)
  estiloGeneral=("${estilo4[@]}")
  ;;
*)
  estiloGeneral=("${estilo4[@]}")
  ;;
esac
